{
    "builds": [
        {
            "src": "SMS/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambSize": "30mb",
                "runtime": "python3.9"
            }
        }
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"SMS/wsgi.py"
        }
    ]
}